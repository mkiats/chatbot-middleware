{
  "version": "2.0",
  "extensions": {
        "durableTask": {
            "hubName": "TimeoutHub",
            "storageProvider": {
                "connectionStringName": "AzureWebJobsStorage"
            }
        }
    },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "azure.cosmos": "Error",
      "default": "Information",
      "Host.Results": "Warning",
      "Function": "Warning",
      "Host.Aggregator": "Error"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}